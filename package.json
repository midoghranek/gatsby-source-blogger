{
  "name": "@ghranek/gatsby-source-blogger",
  "version": "1.1.3",
  "description": "Gatsby source plugin for building websites using Blogger as a data source",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "blogger",
    "headless-cms"
  ],
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "format": "prettier --single-quote --write src/gatsby-node.js",
    "prepare": "npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/midoghranek/gatsby-source-blogger.git"
  },
  "author": "Marco Valsecchi <marco.valsecchi@gmail.com> (https://marcovalsecchi.it/)",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^40.0.0",
    "rehype-parse": "^6.0.0",
    "rehype-remark": "^6.0.0",
    "remark-stringify": "^6.0.4",
    "unified": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "prettier": "^1.18.2"
  },
  "bugs": {
    "url": "https://github.com/midoghranek/gatsby-source-blogger/issues"
  },
  "homepage": "https://github.com/midoghranek/gatsby-source-blogger#readme"
}
